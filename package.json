{
  "name": "cc-api-docs",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch-js": "budo index.js --live --open -- -t [babelify --presets babel-preset-es2015]",
    "watch-css": "postcss -c postcss.watch.js -w -l",
    "build-css": "postcss -c postcss.build.js -m file -l",
    "build-js": "browserify index.js -o dist/index.js -t [babelify --presets es2015] && uglifyjs dist/index.js -o dist/index.js --source-map dist/index.js.map -c -m -v",
    "prepublish": "mkdir -p dist && cp index.html dist/ && npm run build-css && npm run build-js"
  },
  "author": "jay r bolton <jayrbolton@gmail.com> (http://jayrbolton.com/)",
  "license": "UNLICENSED",
  "dependencies": {
    "ff-core": "0.0.4",
    "flyd": "0.2.2",
    "flyd-ajax": "0.1.0",
    "flyd-url": "0.0.1",
    "pretty-js": "^0.1.8",
    "ramda": "0.22.1",
    "snabbdom": "0.5.3",
    "url": "0.11.0"
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "css-mother-starter": "0.0.21",
    "cssnano": "3.7.5",
    "postcss-cli": "2.6.0",
    "postcss-import": "8.1.2",
    "uglify-js": "2.7.3",
    "babel-preset-es2015": "6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0"
  }
}
